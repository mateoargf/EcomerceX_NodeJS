    }

    .descuentoLink:hover span:nth-child(4) {
        top: 0;
    }

    .containerNav a::before {
        content: "";
        position: absolute;
        width: 100%;
        height: 1px;
        border-radius: 4px;
        background-color: #000;
        bottom: 0;
        left: 0;
        transform-origin: right;
        transform: scaleX(0);
        transition: all 0.5s ease-in-out;
    }

    .containerNav a:hover::before,
    .containerNav a:focus::before {
        transform-origin: left;
        transform: scaleX(1);

    }

    .search:hover,
    .carrito:hover,
    .menuIcon:hover {
        border: 2px solid #4a4a4a7c;
    }

