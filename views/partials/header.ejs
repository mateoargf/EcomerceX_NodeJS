    .search:focus-within input::placeholder,
    .carrito:focus-within i,
    .menuIcon:focus-within i {
        color: #080808;
        transition: all 0.3s ease-in-out;
    }

    .search:focus-within input::placeholder {
        transition: all 0.3s ease-in-out;
    }

